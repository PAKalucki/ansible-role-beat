filebeat.config.inputs:
  path: ${path.config}/conf.d/*.yml
  reload.enabled: false

filebeat.config.modules:
  path: ${path.config}/modules.d/*.yml
  reload.enabled: false

tags: ["filebeat"]

output.logstash:
  enabled: true
  hosts: [{{ beat_logstash_url }}]
  loadbalance: true
  ttl: 3600s
  ssl.enabled: true
  ssl.key_passphrase: "${KEY}"
  ssl.verification_mode: full #none

logging.level: info
logging.metrics.enabled: false